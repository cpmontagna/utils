pages = {303--312},
title = {{Identifying elements of the plumbing system beneath Kilauea Volcano, Hawaii, from the source locations of very-long-period signals}},
doi = {10.1046/j.1365-246X.2002.01629.x},
volume = {148},
year = {2002}
}

@article{Cayol:2000hf,
abstract = {After a magnitude 7.2 earthquake in 1975 and before the start of the ongoing eruption in 1983, deformation of Kilauea volcano was the most rapid ever recorded. Three-dimensional numerical modeling shows that this deformation is consistent with the dilation of a dike within Kilauea’s rift zones coupled with creep over a narrow area of a low-angle fault beneath the south flank. Magma supply is estimated to be 0.18 cubic kilometers per year, twice that of previous estimates. The 1983 eruption may be a direct consequence of the high rates of magma storage within the rift zone that followed the 1975 earthquake.},
annote = {hawaii/21},
author = {Cayol, V. and Dieterich, J. H. and Okamura, A. T. and Miklius, A.},
